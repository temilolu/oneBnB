{
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier":  ["error", {"singleQuote": true, "parser": "flow"}]
    },
    "extends": [
        "airbnb",
        "prettier",
        "prettier/react"
      ],
      "parserOptions" : {
        "ecmaVersion": 2016,
        "sourceType" : "module",
        "ecmaFeatures": {
          "jsx": true
        }
      },
      "env": {
        "es6": true,
        "browser": true,
        "node": true
      }
  } 